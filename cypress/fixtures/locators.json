{
    "header": {
        "loginButton": "a[href='/login']",
        "registerButton": "a[href='/register']",
        "logoutButton": "a[href='#']",
        "professors": {
            "professorsDropdown": "#navbardrop",
            "allProfessorsButton": "a[href='/all-professors']",
            "createProfessor": "a[href='/create-professor']"
        },
        "createGradebook": "a[href='/create-gradebook']",
        "myGradebook": ".nav-link"
    },
    "register": {
        "formTitle": "h2",
        "firstName": "#firstName",
        "lastName": "#lastName",
        "password": "#password",
        "confirmPassword": "#passwordConfirmation",
        "email": "#email",
        "checkboxTerms": "#termsAndConditions",
        "buttonSubmit": "button[type='submit']"
    },
    "login": {
        "email": "input[name='email']",
        "password": "input[name='password']",
        "buttonSubmit": "button[type='submit']"
    },
    "homePageGradebooks": {
        "pageTitle": "h3",
        "table": "table",
        "noDiariesMessage": "p",
        "inputFilter": "input[type='text']",
        "buttonSearch": ".btn"
    },
    "allProfessors": {
        "pageTitle": "h3",
        "filterField": "input[type='text']"
    },
    "createGradebook": {
        "pageTitle": "h3",
        "title": "#title",
        "professor": "#professor",
        "buttonSubmit": ".btn"
    },
    "createProfessor": {
        "pageTitle": "h3",
        "firstName": "#firstName",
        "lastName": "#lastName",
        "addImageButton": ".btn-sm",
        "addImageInput": "input[placeholder='Image URL']",
        "buttonSubmit": "button[type='submit']"
    },
    "myGradebook": {
        "pageTitle": "h3",
        "gradebookTitle": "tbody > tr > :nth-child(2)",
        "professorName": "tbody > tr > :nth-child(3)",
        "studentsList": "tbody > tr > :nth-child(4)",
        "studentName": ":nth-child(4) > ul > li",
        "addNewStudent": {
            "addStudentButton": ".btn-primary",
            "noDiaryValidation": "p"
        }
    }
}